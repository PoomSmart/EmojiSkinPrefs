        <string>EmojiSkinPrefs.png</string>
        <key>label</key>
        <string>EmojiSkinPrefs</string>
    </dict>
    <key>items</key>
    <array>
        <dict>
            <key>cell</key>
            <string>PSGroupCell</string>
            <key>footerAlignment</key>
            <integer>1</integer>
            <key>footerText</key>
            <string>© 2025 PoomSmart</string>
        </dict>
        <dict>
            <key>cell</key>
            <string>PSLinkListCell</string>
            <key>default</key>
            <integer>0</integer>
            <key>defaults</key>
            <string>com.apple.UIKit</string>
            <key>detail</key>
            <string>PSListItemsController</string>
            <key>key</key>
            <string>SkinNum</string>
